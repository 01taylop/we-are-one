{
  "env": {
    "browser": true,
    "es6": true,
    "jest/globals": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "import",
    "jest",
    "react",
    "wyze"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "arrow-body-style": [2, "as-needed"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "babel/new-cap": 0,
    "babel/camelcase": 0,
    "babel/no-invalid-this": 0,
    "babel/object-curly-spacing": 0,
    "babel/quotes": 0,
    "babel/semi": 0,
    "babel/no-unused-expressions": 0,
    "babel/valid-typeof": 0,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": [2, {
      "properties": "never"
    }],
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "constructor-super": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "func-call-spacing": 2,
    "function-paren-newline": [2, "consistent"],
    "id-length": 0,
    "implicit-arrow-linebreak": 2,
    "indent": [2, 2],
    "jsx-quotes": [2, "prefer-single"],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-between-class-members": 2,
    "max-depth": [2, 4],
    "max-nested-callbacks": [2, 4],
    "max-statements-per-line": 2,
    "multiline-comment-style": [2, "starred-block"],
    "new-cap": 2,
    "no-alert": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-confusing-arrow": 2,
    "no-console": 0,
    "no-div-regex": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-extra-parens": [2, "all", {
      "ignoreJSX": "all",
      "nestedBinaryExpressions": false,
      "returnAssign": false
    }],
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2, {
      "allow": ["!!"]
    }],
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-operators": 2,
    "no-multi-assign": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, {
      "max": 1
    }],
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-plusplus": 2,
    "no-proto": 2,
    "no-prototype-builtins": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 2,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {
      "ignoreRestSiblings": true
    }],
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-curly-newline": [2, {
      "consistent": true
    }],
    "object-curly-spacing": [2, "always"],
    "object-shorthand": [2, "always"],
    "operator-assignment": 2,
    "padded-blocks": [2, {
      "blocks": "never",
      "classes": "always",
      "switches": "never"
    }],
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-destructuring": [2, {
      "array": false,
      "object": true
    }],
    "prefer-promise-reject-errors": 2,
    "prefer-template": 2,
    "quote-props": [2, "consistent-as-needed"],
    "quotes": [2, "single"],
    "require-await": 2,
    "rest-spread-spacing": 2,
    "semi": [2, "never"],
    "semi-spacing": 2,
    "semi-style": 2,
    "sort-keys": [2, "asc", {
      "caseSensitive": true,
      "natural": true
    }],
    "sort-vars": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, {
      "anonymous": "never",
      "asyncArrow": "always",
      "named": "never"
    }],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [2, "always", {
      "exceptions": ["-"]
    }],
    "strict": [2, "global"],
    "switch-colon-spacing": 2,
    "template-curly-spacing": 2,
    "template-tag-spacing": 2,
    "wrap-regex": 2,

    // import
    "import/default": 0,
    "import/export": 2,
    "import/exports-last": 0,
    "import/extensions": [1, "never", {
      "scss": "always",
      "json": "always"
    }],
    "import/first": [2, "absolute-first"],
    "import/group-exports": 0,
    "import/max-dependencies": 1,
    "import/named": 2,
    "import/namespace": 2,
    "import/newline-after-import": 2,
    "import/no-absolute-path": 2,
    "import/no-amd": 2,
    "import/no-anonymous-default-export": 2,
    "import/no-commonjs": 2,
    "import/no-deprecated": 2,
    "import/no-dynamic-require": 2,
    "import/no-extraneous-dependencies": 2,
    "import/no-mutable-exports": 2,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 2,
    "import/no-named-default": 2,
    "import/no-namespace": 2,
    "import/no-relative-parent-imports": 0,
    "import/no-self-import": 2,
    "import/no-unassigned-import": [2, {
      "allow": ["**/*.css", "**/*.sass", "**/*.scss"]
    }],
    "import/no-unresolved": 2,
    "import/no-unused-modules": 2,
    "import/no-useless-path-segments": 2,
    "import/no-webpack-loader-syntax": 2,
    "import/order": [2, {
      "newlines-between": "always"
    }],
    "import/prefer-default-export": 0,
    "import/unambiguous": 1,
    "yoda": 2,

    // react
    "react/boolean-prop-naming": [2, {
      "rule": "^(is|has)[A-Z]([A-Za-z0-9]?)+"
    }],
    "react/button-has-type": 2,
    "react/default-props-match-prop-types": 2,
    "react/destructuring-assignment": 0,
    "react/display-name": 0,
    "react/forbid-component-props": 0,
    "react/forbid-dom-props": 2,
    "react/forbid-elements": 0,
    "react/forbid-foreign-prop-types": 0,
    "react/forbid-prop-types": 2,
    "react/jsx-boolean-value": 2,
    "react/jsx-child-element-spacing": 2,
    "react/jsx-closing-bracket-location": 2,
    "react/jsx-closing-tag-location": 2,
    "react/jsx-curly-brace-presence": 2,
    "react/jsx-curly-spacing": [2, "never"],
    "react/jsx-equals-spacing": [2, "never"],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "react/jsx-first-prop-new-line": [2, "multiline"],
    "react/jsx-fragments": [2, "element"],
    "react/jsx-handler-names": [2, {
      "eventHandlerPrefix": "on",
      "eventHandlerPropPrefix": "handle"
    }],
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-max-depth": 0,
    "react/jsx-max-props-per-line": [2, { "maximum": 1, "when": "multiline" }],
    "react/jsx-no-bind": [2, {
      "ignoreRefs": true
    }],
    "react/jsx-no-literals": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-pascal-case": 2,
    "react/jsx-props-no-multi-spaces": 2,
    "react/jsx-sort-default-props": 2,
    "react/jsx-sort-props": 0,
    "react/jsx-tag-spacing": [2, {
      "afterOpening": "never",
      "beforeSelfClosing": "never",
      "closingSlash": "never"
    }],
    "react/jsx-wrap-multilines": 2,
    "react/no-access-state-in-setstate": 2,
    "react/no-array-index-key": 0,
    "react/no-danger": 2,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-find-dom-node": 2,
    "react/no-multi-comp": 2,
    "react/no-redundant-should-component-update": 2,
    "react/no-set-state": 0,
    "react/no-this-in-sfc": 2,
    "react/no-typos": 2,
    "react/no-unused-prop-types": 2,
    "react/no-unused-state": 2,
    "react/no-will-update-set-state": 2,
    "react/prefer-es6-class": 2,
    "react/prefer-stateless-function": [2, {
      "ignorePureComponents": true
    }],
    "react/prop-types": [2, {
      "ignore": [
        "children",
        "className"
      ]
    }],
    "react/react-in-jsx-scope": 0,
    "react/require-default-props": 0,
    "react/require-optimization": 0,
    "react/self-closing-comp": [2, {
      "component": true,
      "html": false
    }],
    "react/sort-comp": 2,
    "react/sort-prop-types": 2,
    "react/style-prop-object": 2,
    "react/void-dom-elements-no-children": 2,

    // wyze
    "wyze/func-call-arg-spacing": 2,
    "wyze/func-params-spacing": 0,
    "wyze/max-file-length": 0,
    "wyze/newline-after-export": 2,
    "wyze/sort-destructuring-keys": 2,
    "wyze/sort-imports": 0,
    "wyze/space-around-conditional": 0
  },
  "overrides": []
}
